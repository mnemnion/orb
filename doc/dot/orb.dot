    u.freeze(\"no\")
    --invert:invert_all(pwd)
elseif verb == \"knit\" then
    rootCodex:spin()
    knit.knitCodex(rootCodex)
elseif verb == \"weave\" then
    weave:weave_all(pwd)
elseif verb == \"spec\" then
    Spec()
elseif verb == \"serve\" then
    -- perform a full cycle
    rootCodex:spin()
    knit.knitCodex(rootCodex)
    weave:weave_all(pwd)
    -- watch for changes
    rootCodex:serve()
    rootCodex.server:run()

elseif not verb then
    -- do the things

    rootCodex:spin()
    knit.knitCodex(rootCodex)
    weave:weave_all(pwd)
end

